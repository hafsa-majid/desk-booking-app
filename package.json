{
  "name": "desk-booking-app",
  "version": "1.0.0",
  "description": "Desk Booking Application with CI/CD pipeline",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=json --coverageReporters=lcov",
    "build": "echo 'Build completed' && mkdir -p build && cp -r . build/",
    "security:audit": "npm audit",
    "security:check": "npx better-npm-audit audit"
  },
  "author": "Majid",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.16.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  }
}
