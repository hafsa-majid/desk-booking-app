{
  "name": "desk-booking-app",
  "version": "1.0.0",
  "description": "Production-grade desk booking system with CI/CD, testing, and security scanning",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=json --coverageReporters=lcov",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "build": "mkdir -p build && cp -r index.js package.json package-lock.json build/",
    "security:audit": "npm audit",
    "security:check": "npx better-npm-audit audit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "express-rate-limit": "^7.5.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "@cyclonedx/cyclonedx-npm": "^1.20.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": ["desk booking", "CI/CD", "Render", "Express", "Node.js"],
  "author": "Hafsa Majid",
  "license": "MIT"
}
